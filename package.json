{
  "name": "multi-sync",
  "displayName": "Multi Sync",
  "description": "Automatically syncs VS Code and Cursor config files using the multi CLI.  Replacement for VS Code multi-root workspaces.",
  "version": "0.0.1",
  "publisher": "montaguegabe",
  "repository": {
    "type": "git",
    "url": "https://github.com/montaguegabe/multi-extension"
  },
  "icon": "icon.png",
  "license": "MIT",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {},
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "install-extension": "vsce package --allow-missing-repository && cursor --install-extension multi-sync-0.0.1.vsix"
  },
  "devDependencies": {
    "@types/vscode": "^1.99.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}
